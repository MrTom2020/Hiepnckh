<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tranghienthi"
    android:id="@+id/rl">

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/bc"
        android:layout_width="393dp"
        android:layout_height="299dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="133dp" />

    <TextView
        android:id="@+id/txtnd"
        android:layout_width="141dp"
        android:layout_height="133dp"
        android:layout_above="@+id/bc"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/txtda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/bc"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="97dp"
        android:layout_toEndOf="@+id/txtnd"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/txtda"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="17dp"
        android:layout_toEndOf="@+id/txtnd"
        android:text="TextView" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="127dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-7dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="-6dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:orientation="vertical">
            <CheckBox
                android:id="@+id/c1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <CheckBox
                android:id="@+id/c2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <CheckBox
                android:id="@+id/c3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/c4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/c5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/c6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <CheckBox
                android:id="@+id/c7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <Button
                android:id="@+id/btnmdbui"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="PM 2.5 " />

            <Button
                android:id="@+id/btnmdkk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="AQI" />
        </LinearLayout>
    </ScrollView>

    <ImageButton
        android:id="@+id/imgluu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="9dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="104dp"
        android:src="@drawable/savve" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="113dp"
        android:layout_height="111dp"
        android:layout_above="@+id/txtttht"
        android:layout_marginStart="23dp"
        android:layout_marginBottom="14dp"
        android:layout_toEndOf="@+id/textView9"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txtttht"
        android:layout_width="183dp"
        android:layout_height="53dp"
        android:layout_above="@+id/bc"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="11dp"
        android:text="TextView" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="56dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="6dp"
        android:src="@drawable/broom" />

</RelativeLayout>